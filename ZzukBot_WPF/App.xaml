<wpf:WpfApplication x:ClassModifier="internal" x:Class="ZzukBot.App"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:wpf="clr-namespace:ZzukBot.WPF">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="WPF/Dictionaries/Converters.xaml" />
                <ResourceDictionary Source="WPF/Dictionaries/Commands.xaml" />
                <ResourceDictionary Source="WPF/Dictionaries/LayoutResources.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</wpf:WpfApplication>